;
; BIND reverse data file for local loopback interface
;
$TTL	604800
@	IN	SOA	webserver.ex. root.webserver.ex. (
			      3		; Serial
			 604800		; Refresh
			  86400		; Retry
			2419200		; Expire
			 604800 )	; Negative Cache TTL
;
@       IN      NS      localhost.
10.2 	IN 	PTR 	nhm.local
10.10	IN	PTR	honeypot01.local
10.11	IN	PTR	honeypot02.local
